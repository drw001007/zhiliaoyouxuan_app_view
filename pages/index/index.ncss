	/* 导航 */
	.homnav{width:100%;margin:0 auto;display: flex;flex-wrap: wrap;position: relative;background: #fff;padding-top: 20upx; }
	.homnav swiper{width: 100%;height: 200upx;position: relative;}
	.homnav swiper .swiper-item{width: 100%;display: flex;justify-content: space-around;}
	.homnav swiper .swiper-item .nli{width: 20%;margin-bottom: 20upx;margin-top:20upx;}
