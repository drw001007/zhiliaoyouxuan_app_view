	.homnav swiper .swiper-item .nli view{color:#333;font-size:24upx;text-align: center;margin-top:10upx;}
	.homnav .dots { position: absolute; bottom: 20rpx; left: 50%;   transform: translate(-50%, 0); -webkit-transform: translate(-50%, 0); z-index: 9; display: flex;  flex-direction: row; justify-content: center;  }
	.homnav .dots  .dot { width: 24rpx;  height: 8rpx;  transition: all .6s; background:#ccc; }
	.homnav .dots .active { width: 24rpx; height: 8rpx;  background: #ff3333;}
	.homnav swiper .uni-swiper-dot-active{background: #ff3333;}
