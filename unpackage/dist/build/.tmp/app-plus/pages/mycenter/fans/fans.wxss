
.header{position: fixed;left: 0;top: 0;width: 94%;z-index:9;padding:0 3%;background: #fff;height: 96rpx;line-height: 96rpx;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;-ms-flex-pack: justify;justify-content: space-between;}
.header .goto{font-size:36rpx;color:#333}
.header .head-titl{width: 500rpx;text-align: center;font-size:26rpx;color:#000;line-height: 96rpx;}
.header	.serar{font-size: 36rpx;color:#333;}
.fantab{width:100%;height: 96rpx;line-height: 96rpx;margin-top:96rpx;border-top:1px solid #efefef;position: relative;}
.fantab-ul{width: 100%;}
.fantab .fantab-ul .tnav{display: inline-block;width: 25%;text-align: center;font-size:26rpx;}
.fantab .fantab-ul .tnav.active{color:#ff3333;}
.fantab .fantab-ul .navmore{display: inline-block;width: 25%;text-align: center;font-size:26rpx;position: relative;}
.fantab .fantab-ul .navmore .arrow{ position: absolute;right:10rpx; top:45rpx; /* left:80rpx; */ width:0;  height:0; margin-left: 10rpx; border-width: 12rpx; 
border-style: solid; border-color:#8f8f8f transparent transparent transparent;}
.ismore{color:#f33;}
.sotview{width: 20%; background: #fff;text-align: center;float: right;margin-right:10rpx;margin-top:-10rpx;position: absolute;right: 0;z-index: 9;}
.sotnav{font-size:24rpx;color:#333;height:60rpx;line-height: 60rpx;}
.sotnav.select{color:#f33}
.fantips{width: 94%;padding:0 3%;background:#ffeaea;color:#e7161a;height: 80rpx;line-height: 80rpx;font-size:24rpx;}
.fantips text{font-size:26rpx;margin-right:20rpx;}
.fantit{width:94%;padding:0 3%;background: #f4f4f4;}
.fantit-one{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;-ms-flex-pack: justify;justify-content: space-between;height:80rpx;position: relative;}
.fantit .fantit-one .fantit-left{color:#333333;font-size:24rpx;line-height: 80rpx;}
.fantit .fantit-one .fantit-left text{color:#f33;padding:0 5rpx;}
.fantit .fantit-one .fantit-right{line-height: 80rpx;color:#333;font-size:24rpx;}
.fantit .fantit-one .fantit-right text{font-size:26rpx;padding-left: 10rpx;}
.fantit .fantwo{width: 100rpx;float: right;margin-right:0rpx;background: #fff;text-align: center;position: absolute;right: 10rpx;}
.fantit .fantwo .scre{height:60rpx;line-height: 60rpx;font-size:24rpx;color:#333;}
.fantit .fantwo .scre.act{color:#f33;}
.fanview{width:100%;}
.fanview .fli{width:94%;padding:0 3%;background: #fff;border-bottom:10rpx solid #f0f0f0;padding-top:40rpx;}
.fanview .fli .li_one{width: 100%;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;-ms-flex-pack: justify;justify-content: space-between;}
.fanview .fli .li_one .lione-lef{width: 500rpx;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;}
.fanview .fli .lione-lef .lione-heic{width:83rpx;height:83rpx;border-radius: 83rpx;}
.fanview .fli .lione-lef .lione-heic image{width:100%;height:100%;border-radius: 100%;}
.fanview .fli .lione-lef .lione-name{margin-left:20rpx;}
.fanview .fli .li_one .lione-name .lione-name-p{color:#333;font-size:30rpx;}
.fanview .fli .li_one .lione-name .lione-name-t{color:#9a9a9a;font-size:24rpx;margin-top:10rpx;}
.fanview .fli .li_one .lione-rig{height:80rpx;line-height: 80rpx;color:#333;font-size:36rpx;font-weight: bold;}
.fanview .fli .li-two{width: 100%;padding:40rpx 0;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack:end;-webkit-justify-content:flex-end;-ms-flex-pack:end;justify-content:flex-end;}
.fanview .fli .litwo_dd{width: 25%;text-align: center;}
.fanview .fli .litwo_dd .litwo_da{color:#333;font-size:24rpx;text-align: center;}
.fanview .fli .litwo_dd .litwo_me{color:#000;font-weight: bold;font-size:30rpx;text-align: center;margin-top:10rpx;}
.fanview .fli .li-free{width: 100%;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;-ms-flex-pack: justify;justify-content: space-between;padding-bottom: 40rpx;
}
.fanview .fli .li-free .lifree-tran{color:#333333;font-size:24rpx;}
.fanview .fli .li-free .lifree-tran text{width: 150rpx;height:15rpx;background: #ebebeb;border-radius: 15rpx;display: inline-block;margin-left:15rpx;}
.fanview .fli .li-free .lifree-rig{color:#333333;font-size:24rpx;}
.backbg{width: 100%;height:100%;position: fixed;left: 0;top: 0;background: rgba(0,0,0,0.8);z-index: 99;}
.box2{position: absolute;top:20%;width: 94%;left:3%;z-index: 100;}
.box2-ts{background: #fff;border-radius: 40rpx;width:100%;}
.box2 .boxcont{background: #fc2e5e;padding:20rpx 0;border-radius: 40rpx 40rpx 0 0;color:#fff;font-size:24rpx;text-align: center;}
.boxcont_name{font-size:24rpx;
}
.boxcont_tel{font-size:24rpx;margin-top:20rpx; text-align: center;}
.boxcont_tel view{display: inline-block;margin-left:20rpx;text-decoration: underline;}
.box-ye{width:600rpx;background: #f3f3f3;padding:20rpx 0;margin: 40rpx auto;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-justify-content: space-around;-ms-flex-pack: distribute;justify-content: space-around;text-align: center;}
.box-ye .ye_ts{color:#333;font-size:24rpx;}
.box-ye .ye-me{color:#fd214b;font-size:26rpx;font-weight: 600;margin-top:10rpx;}
.box-fres{width: 600rpx;padding:20rpx 0;margin: 0 auto;}
.box-fres_l{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;-ms-flex-pack: justify;justify-content: space-between;margin-bottom: 20rpx;}
.box-fres_l .free_left{color:#333333;font-size:26rpx;}
.box-fres_l .free_rig{color:#333333;font-size:26rpx;}
.close{color:#Fff;font-size:46rpx;text-align: center;margin-top:40rpx;}
