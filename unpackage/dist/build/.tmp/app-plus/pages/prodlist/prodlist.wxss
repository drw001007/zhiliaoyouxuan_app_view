
.searchbox{width:100%;background:#fff;position: fixed;left: 0;top: 0;z-index: 999;
}
.searchbox .searlist{width:94%;padding:0 3%;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;}
.searchbox .goto{font-size:36rpx;color:#333;height:100rpx;line-height: 100rpx;}
.searchbox .searview{width: 600rpx;height: 60rpx;margin:20rpx auto; background: #eeeeee; 	border-radius: 10rpx;}
.searchbox .searview icon{ 	margin-left: 10rpx;	float: left;margin-top:10rpx;}
/* .searchbox .searview input{float: left;width: 370upx;height:60upx;line-height: 60upx;	color:#333;font-size:26upx;margin-left: 15upx;}
.searchbox .searview .middcose{width: 50upx;height: 60upx;float: left;} */
.searchbox .setaxt{font-size:24rpx;height: 60rpx;line-height: 60rpx;color:#999999;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;margin-left:20rpx;float: left;}
.searchbox .setaxt view{color:#333;}
.searchbox .searview .middsear{float: right;width: 110rpx;color: #fff;background:-webkit-gradient(linear,left top, right top,from(#e74766),to(#fe3435)) ;background:-o-linear-gradient(left,#e74766,#fe3435) ;background:linear-gradient(to right,#e74766,#fe3435) ;text-align: center;line-height: 60rpx;border-top-right-radius: 10rpx;border-bottom-right-radius: 10rpx;letter-spacing:4rpx;font-size:30rpx;}
.classtabs{width: 94%;margin: 0 auto;border-top:10rpx solid #f2f2f2;padding:30rpx  3%;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-justify-content: space-around;-ms-flex-pack: distribute;justify-content: space-around;}
.classtabs  .twli{display: inline-block;background: #f2f2f2;color:#333;font-size: 26rpx;border-radius: 20rpx;padding:5rpx 20rpx;}
.classtabs  .twli.active{background: #fae4e4;color:#fd3538;}
.twogood{width: 100%;}
.mtop{margin-top:220rpx;}
.proul{margin-top:150rpx;}
.proul .proul_li{width:94%;margin:0 auto;padding:20rpx 0; -webkit-box-shadow: 2rpx 2rpx 20rpx #efefef; box-shadow: 2rpx 2rpx 20rpx #efefef;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;margin-bottom:20rpx;}
.proul .proul_li .prod_img{width: 260rpx;height: 260rpx;}
.proul .proul_li .prod_img image{width: 100%;height: 100%;}
.proul .proul_li .prod_cent{width:400rpx;margin-left:20rpx;}
.proul .proul_li .prod_cent .prod_name{ color:#333;font-size:30rpx;font-weight: 600;overflow: hidden;white-space: normal;-webkit-line-clamp:2;
word-wrap: break-word;-webkit-box-orient: vertical;height:100rpx;line-height:50rpx;}
.proul .proul_li .prod_cent .prod_name image{width:35rpx;height:35rpx;float:left;margin-top:8rpx;margin-right:10rpx;
}
/*.proul .proul_li .prod_cent .prod_name view image{width:100%;height:100%; }*/
.proul .proul_li .prod_cent .prod_yjsy{display:inline-block;color:#fff;font-size:24rpx;padding:5rpx 20rpx;border-radius: 0 20rpx 20rpx 0; background: -webkit-gradient(linear,left top, right top,from(#fca65c),to(#e9729d)); background: -o-linear-gradient(left,#fca65c,#e9729d); background: linear-gradient(to right,#fca65c,#e9729d);}
.proul .proul_li .prod_cent .prod_one{display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;-webkit-box-pack:justify;-webkit-justify-content:space-between;-ms-flex-pack:justify;justify-content:space-between;margin-top:20rpx;}
.proul .proul_li .prod_cent .prod_one .prod_mark{color:#999999;font-size:22rpx;text-decoration:line-through;
}
.proul .proul_li .prod_cent .prod_one .prod_mouth{color:#999;font-size:22rpx;}
.proul .proul_li .prod_cent .prod-up{margin-top:20rpx;display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;-webkit-box-pack:justify;-webkit-justify-content:space-between;-ms-flex-pack:justify;justify-content:space-between;}
.proul .proul_li .prod_cent .prod-up .prod-up_shop{color:#fe3436;font-size:24rpx;}
.proul .proul_li .prod_cent .prod-up .prod-up_shop text{font-size:32rpx;font-weight:600;}
.proul .proul_li .prod_cent .prod_up-coup{position:relative; background:#fe3436;color:#fff;padding:5rpx 20rpx;height:40rpx;line-height:40rpx;border-radius:5rpx;}
.proul .proul_li .prod_cent .prod_up-coup .prod_up_ld{position:absolute;left:-10rpx;top:15rpx;}
.proul .proul_li .prod_cent .prod_up-coup .prod_up_rd{position:absolute;right:-10rpx;top:15rpx;}
.proul .proul_li .prod_cent .prod_up-coup .prod_dd{width:20rpx;height:20rpx;border-radius:20rpx;background:#fff;}
.proul .proul_li .prod_cent .prod_up-coup .prod_up-ts{text-align:center;font-size:26rpx;}
/*底部*/
.medix{width:100%;height:80rpx;line-height:80rpx;text-align:center;font-size:24rpx;color:#666;}
/*加载*/
.loaddiv{width:64rpx;height: 64rpx;margin: 0 auto;margin-top:260rpx;}
.loaddiv image{width: 100%;height: 100%;}
.noData{width:160rpx;height: 160rpx;margin: 0 auto;margin-top:400rpx;}
.noData image{width: 100%;height: 100%;}
.liloading{width:64rpx;height: 64rpx;margin: 50rpx auto;}
.liloading image{width: 100%;height: 100%;}
/* 返回顶部 */
.gotop{position: fixed;right: 30rpx;bottom: 40rpx;z-index: 99;}
.gotop text{font-size:30rpx;color:#999;}

