
page{background: #f5f5f5;}
.header{position: fixed;left: 0;top: 0;width: 94%;padding:0 3%;background: #fff;height: 96rpx;line-height: 96rpx;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;}
.header .goto{font-size:36rpx;color:#333}
.header .head-titl{width: 100%;text-align: center;font-size:30rpx;color:#000;line-height: 96rpx;}
.ordhead{width: 100%;position: fixed;height: 90rpx;left: 0;top: 100rpx;background: #fff;z-index: 999;}
.ordhead .navul{width: 100%;margin:0 auto;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;}
.ordhead .navul .nav{width: 34%;line-height:88rpx;border-bottom:2rpx solid #fff;color:#333;font-size:26rpx;text-align: center;}
.ordhead .navul .nav.active{color:#f33;border-bottom:2rpx solid #f33;}
.ordview{margin-top:200rpx;width: 100%;}
.ordview .ordvul .dli{width: 94%;padding:0 3%;background: #fff;margin-bottom:15rpx;padding-bottom: 20rpx;}
.ordview .ordvul .dli .dli_one{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;-ms-flex-pack: justify;justify-content: space-between;border-bottom: 1px dashed #e8e8e8;padding:16rpx 0;}
.ordview .ordvul .dli .dli_one .dli_onehm{font-size: 24rpx;color: #999;letter-spacing: 2rpx;}
.ordview .ordvul .dli .dli_two{width: 94%;padding:15rpx 3%;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;}
.ordview .ordvul .dli .dli_two .ord_tu{width: 160rpx;height: 160rpx;border: solid 1px #fff;border-radius: 20rpx;margin-top: 10rpx;}
.ordview .ordvul .dli .dli_two .ord_tu image{width: 100%;height:100%;}
.ordview .ordvul .dli .dli_two .ord-righ{width:520rpx;margin-left:20rpx;margin-top:20rpx;}
.ordview .ordvul .dli .dli_two .ord-righ .ord_name{font-size:26rpx;margin-top:10rpx;color: #333;overflow: hidden;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;letter-spacing: 2rpx;}
.ordview .ordvul .dli .dli_two .ord-righ .ord_time{color:#999;font-size:24rpx;margin-top:15rpx;}
.ordview .ordvul .dli .ord_infor{margin-top:10rpx; display: -webkit-box; display: -webkit-flex; display: -ms-flexbox; display: flex; text-align: center;-webkit-box-pack: end;-webkit-justify-content: flex-end;-ms-flex-pack: end;justify-content: flex-end;text-align: right;}
.ordview .ordvul .dli .ord_infor .it{width: 200rpx;}
.ordview .ordvul .dli .ord_infor .it .it_bbq{color: #f60;font-size: 28rpx;}
.ordview .ordvul .dli .ord_infor .it .it_text{color: #666;font-size: 26rpx;}
/*加载*/
.loaddiv{width:64rpx;height: 64rpx;margin: 50rpx auto;}
.loaddiv image{width: 100%;height: 100%;}
.noData{width:160rpx;height: 160rpx;margin: 0 auto;margin-top:400rpx;}
.noData image{width: 100%;height: 100%;}
.liloading{width:64rpx;height: 64rpx;margin: 50rpx auto;}
.liloading image{width: 100%;height: 100%;}
.medix{width:100%;height:80rpx;line-height:80rpx;text-align:center;font-size:24rpx;color:#666;}

