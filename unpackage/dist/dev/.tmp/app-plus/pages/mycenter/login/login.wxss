.regis .reli_yam .getyam{width: 150rpx;height: 50rpx;background: #f33;color:#fff;font-size:24rpx;text-align: center;line-height: 50rpx;padding:0;margin:0;border:0;}
.regis .reli_yam .getgray{width: 150rpx;height: 50rpx;background: #999;color:#fff;font-size:24rpx;text-align: center;line-height: 50rpx;padding:0;margin:0;border:0;}
.regis  .rebtn{width: 600rpx;height: 90rpx;background: #ff3333;color:#fff;font-weight: 500;font-size: 40rpx;border-radius: 90rpx;line-height: 90rpx;margin-top:100rpx;}
.regis .forget{color:#ff3333;font-size:26rpx;text-align: center;padding-top:20rpx;letter-spacing: 2rpx;}
