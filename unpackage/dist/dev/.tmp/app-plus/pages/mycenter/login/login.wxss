.regis{width:600rpx;margin: 0 auto;padding:40rpx 50rpx;background: #fff;-webkit-box-shadow: 2rpx 2rpx 20rpx #fbc5bd;box-shadow: 2rpx 2rpx 20rpx #fbc5bd;position: relative;z-index: 999;border-radius: 40rpx;}
.regis .regis_tit{font-size:56rpx;color:#444;}
.regis .boxinput{margin-top:60rpx;}
.regis .reli{height: 100rpx;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;border-bottom: 1px solid #cccccc;}
.regis .reli .reli_icon{width: 45rpx;line-height: 100rpx;color:#ff3333;text-align: center;}
.regis .reli .reli_it{width: 500rpx;height: 70rpx;margin-top: 15rpx;margin-left:20rpx;}
.regis .reli .reli_it input{width: 100%;height:70rpx;line-height: 70rpx;color:#333;font-size:26rpx;}
